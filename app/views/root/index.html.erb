<h1><%=t('.header')%></h1>
<p class="lead"><%=t('.lead')%></p>

<div class="container-fluid">
  <div class="row">
    <div class="col-md-3">
      <div class="panel panel-default">
        <div class="panel-heading"><%=t('.reports.dkim.header')%></div>
        <div class="panel-body">
          <%=pie_chart(api_dkim_usage_path(:format => :json), {:title => t('.reports.dkim.usage')})%>

          <%=line_chart(api_dkim_usage_overtime_path(:format => :json), {:title => t('.reports.dkim.usage_overtime')})%>
        </div>
      </div>
    </div>
  </div>
</div>
